{
  "name": "Road-Work-Appwork-app",
  "productName": "Road Work App",
  "description": "Instructional Application for use with the ITRE Road Work Safety Training Course",
  "version": "0.0.1",
  "author": "Devin Cremins <drcremin@ncsu.edu>",
  "homepage": "http://octopusoddments.com",
  "license": "MIT",
  "main": "app/background.js",
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "publish": {
      "provider": "github",
      "private": false
    }
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "prerelease": "gulp build --env=production",
    "build": "gulp build",
    "dist": "electron-builder --publish=always -mwl",
    "release": "build",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register"
  },
  "dependencies": {
    "fs-jetpack": "^0.12.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "electron": "1.6.2",
    "electron-builder": "^19.10.0",
    "electron-mocha": "^3.3.0",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-less": "^3.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "istanbul": "^0.4.5",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "rollup": "^0.41.4",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.4.11",
    "spectron": "^3.6.0"
  }
}
